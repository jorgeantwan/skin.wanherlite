<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol>5</defaultcontrol>
    <coordinates>
        <left>360</left>
        <top>104</top>
    </coordinates>
    <include>dialogeffect</include>
    <controls>
        <control type="group">
            <animation effect="fade" start="100" end="0" time="400" condition="Window.IsVisible(SliderDialog)">Conditional</animation>
            <include name="DialogBackgroundCommons">
                <param name="DialogBackgroundWidth" value="1200" />
                <param name="DialogBackgroundHeight" value="872" />
                <param name="DialogHeaderLabel" value="-" />
                <param name="DialogHeaderId" value="2" />
            </include>
            <control type="grouplist" id="5">
                <description>control area</description>
                <left>32</left>
                <top>88</top>
                <width>1136</width>
                <height>825</height>
                <itemgap>0</itemgap>
                <pagecontrol>60</pagecontrol>
                <onright>60</onright>
                <onup>5</onup>
                <ondown>5</ondown>
            </control>
            <control type="scrollbar" id="60">
                <left>1170</left>
                <top>88</top>
                <width>10</width>
                <height>825</height>
                <onleft>5</onleft>
                <onright>5</onright>
                <orientation>vertical</orientation>
            </control>
        </control>
        <control type="button" id="7">
            <align>left</align>
            <height>75</height>
            <font>font15</font>
            <textcolor>$VAR[ColorTextVar]</textcolor>
            <focusedcolor>$VAR[ColorTextVar]</focusedcolor>
            <texturefocus colordiffuse="$VAR[ColorHighlightVar]">listselect_fo.png</texturefocus>
        </control>
        <control type="radiobutton" id="8">
            <height>75</height>
            <font>font15</font>
            <align>left</align>
            <radioposx>1230</radioposx>
            <texturefocus colordiffuse="$VAR[ColorHighlightVar]">listselect_fo.png</texturefocus>
        </control>
        <control type="spincontrolex" id="9">
            <height>75</height>
            <font>font15</font>
            <align>left</align>
            <reverse>yes</reverse>
            <texturefocus colordiffuse="$VAR[ColorHighlightVar]">listselect_fo.png</texturefocus>
        </control>
        <control type="edit" id="12">
            <height>75</height>
            <font>font15</font>
            <align>left</align>
        </control>
        <control type="label" id="14">
            <height>75</height>
            <align>left</align>
            <textoffsetx>15</textoffsetx>
            <textcolor>$VAR[ColorHighlightVar]</textcolor>
            <font>font15_title</font>
        </control>
        <control type="sliderex" id="13">
            <height>75</height>
            <font>font15</font>
            <textcolor>$VAR[ColorTextVar]</textcolor>
            <focusedcolor>$VAR[ColorTextVar]</focusedcolor>
            <texturefocus colordiffuse="$VAR[ColorHighlightVar]">listselect_fo.png</texturefocus>
        </control>
    </controls>
</window>
